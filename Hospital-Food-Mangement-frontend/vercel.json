{
    "version": 2,
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ],
    "env": {
      "DATABASE_URL": "@database_url"
    },
    "regions": ["iad1"],
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/api/$1"
      }
    ]
  }
  